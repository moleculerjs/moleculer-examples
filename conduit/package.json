{
  "name": "moleculer-realworld-example-app",
  "version": "1.1.0",
  "description": "RealWorld example app with Moleculer microservices framework",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services",
    "start": "moleculer-runner",
    "deps": "npm-check -u",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services",
    "docker:build": "docker build -t conduit ."
  },
  "keywords": [
    "microservices",
    "moleculer",
    "realworld"
  ],
  "author": "",
  "devDependencies": {
    "eslint": "4.19.1",
    "jest": "22.4.3",
    "jest-cli": "22.4.3",
    "moleculer-repl": "0.4.0",
    "npm-check": "5.6.0"
  },
  "dependencies": {
    "bcrypt": "1.0.3",
    "ioredis": "3.2.2",
    "jsonwebtoken": "8.2.0",
    "lodash": "4.17.5",
    "moleculer": "0.12.1",
    "moleculer-console-tracer": "0.1.1",
    "moleculer-db": "0.7.2",
    "moleculer-db-adapter-mongo": "0.2.0",
    "moleculer-web": "0.7.0-beta3",
    "nats": "0.8.4",
    "slug": "0.9.1"
  },
  "engines": {
    "node": ">= 6.x.x"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
